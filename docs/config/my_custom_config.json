{
  "log_file_path": "./my_log_file.log",
  "log_method": 0,
  "def_msg_format": "(${TIME}) ${FILE} : ${MSG}",
  "log_levels": [
    {
      "log_to_tty": true,
      "log_to_file": false,
      "color": 0
    },
    {
      "log_to_tty": false,
      "log_to_file": true,
      "color": 0
    },
    {
      "log_to_tty": true,
      "log_to_file": true,
      "color": 2
    },
    {
      "log_to_tty": true,
      "log_to_file": true,
      "color": 6,
      "msg_format": "This log level has overriden message format: ${MSG}"
    },
    {
      "log_to_tty": true,
      "log_to_file": false,
      "color": 5,
      "callback": "log_callback"
    },
    {
      "log_to_tty": true,
      "log_to_file": true,
      "color": 3,
      "msg_format": "This log level has both message format override and a callback: ${MSG}",
      "callback": "log_callback"
    }
  ]
}
